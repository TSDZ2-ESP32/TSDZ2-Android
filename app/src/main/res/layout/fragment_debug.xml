        app:string_title="@string/duty_cycle"
        app:is_need_dial="false"
        app:back_color="@android:color/darker_gray"
        app:current_value="@{(float)tsdz_debug.dutyCycle}"
        />
    -->

    <TextView
        android:id="@+id/dutyTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/duty_cycle"
        android:textAppearance="@android:style/TextAppearance.Material.Medium"
        app:layout_constraintTop_toTopOf="@+id/horiz1"
        app:layout_constraintBottom_toTopOf="@+id/dutyValueTV"
        app:layout_constraintEnd_toStartOf="@+id/vert2"
        app:layout_constraintStart_toStartOf="@+id/vert1" />

    <TextView
        android:id="@+id/dutyValueTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="32sp"
        android:text="@{String.valueOf(tsdz_debug.dutyCycle)}"
        android:textAppearance="@android:style/TextAppearance.Material.Large"
        app:layout_constraintBottom_toTopOf="@+id/horiz2"
        app:layout_constraintEnd_toStartOf="@+id/vert2"
        app:layout_constraintStart_toStartOf="@+id/vert1"
        app:layout_constraintTop_toBottomOf="@+id/horiz1"
        app:layout_constraintVertical_bias="0.6" />

    <TextView
        android:id="@+id/erpsTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/motor_erps"
        android:textAppearance="@android:style/TextAppearance.Material.Medium"
        app:layout_constraintTop_toTopOf="@+id/horiz1"
        app:layout_constraintBottom_toTopOf="@+id/erpsValueTV"
        app:layout_constraintEnd_toStartOf="@+id/vert3"
        app:layout_constraintStart_toStartOf="@+id/vert2" />

    <TextView
        android:id="@+id/erpsValueTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="32sp"
        android:text="@{String.valueOf(tsdz_debug.motorERPS)}"
        android:textAppearance="@android:style/TextAppearance.Material.Large"
        app:layout_constraintBottom_toTopOf="@+id/horiz2"
        app:layout_constraintEnd_toStartOf="@+id/vert3"
        app:layout_constraintStart_toStartOf="@+id/vert2"
        app:layout_constraintTop_toTopOf="@+id/horiz1"
        app:layout_constraintVertical_bias="0.6"/>


    <TextView
        android:id="@+id/focTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/foc_angle"
        android:textAppearance="@android:style/TextAppearance.Material.Medium"
        app:layout_constraintTop_toTopOf="@+id/horiz2"
        app:layout_constraintBottom_toTopOf="@+id/focValueTV"
        app:layout_constraintEnd_toStartOf="@+id/vert2"
        app:layout_constraintStart_toStartOf="@+id/vert1" />

    <TextView
        android:id="@+id/focValueTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="32sp"
        android:text="@{String.valueOf(tsdz_debug.focAngle)}"
        android:textAppearance="@android:style/TextAppearance.Material.Large"
        app:layout_constraintBottom_toTopOf="@+id/horiz3"
        app:layout_constraintEnd_toStartOf="@+id/vert2"
        app:layout_constraintStart_toStartOf="@+id/vert1"
        app:layout_constraintTop_toTopOf="@+id/horiz2"
        app:layout_constraintVertical_bias="0.6" />

    <TextView
        android:id="@+id/torqueTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/torque_adc"
        android:textAppearance="@android:style/TextAppearance.Material.Medium"
        app:layout_constraintTop_toTopOf="@+id/horiz2"
        app:layout_constraintBottom_toTopOf="@+id/torqueValueTV"
        app:layout_constraintEnd_toStartOf="@+id/vert3"
        app:layout_constraintStart_toStartOf="@+id/vert2" />

    <TextView
        android:id="@+id/torqueValueTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="32sp"
        android:text="@{String.valueOf(tsdz_debug.torqueADCValue)}"
        android:textAppearance="@android:style/TextAppearance.Material.Large"
        app:layout_constraintBottom_toTopOf="@+id/horiz3"
        app:layout_constraintEnd_toStartOf="@+id/vert3"
        app:layout_constraintStart_toStartOf="@+id/vert2"
        app:layout_constraintTop_toTopOf="@+id/horiz2"
        app:layout_constraintVertical_bias="0.6"/>


    <TextView
        android:id="@+id/pTorqueTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/pedal_torque"
        android:textAppearance="@android:style/TextAppearance.Material.Medium"
        app:layout_constraintTop_toTopOf="@+id/horiz3"
        app:layout_constraintBottom_toTopOf="@+id/pTorqueValueTV"
        app:layout_constraintEnd_toStartOf="@+id/vert2"
        app:layout_constraintStart_toStartOf="@+id/vert1" />

    <TextView
        android:id="@+id/pTorqueValueTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="32sp"
        android:text="@{String.valueOf(tsdz_debug.pTorque)}"
        android:textAppearance="@android:style/TextAppearance.Material.Large"
        app:layout_constraintBottom_toTopOf="@+id/horiz4"
        app:layout_constraintEnd_toStartOf="@+id/vert2"
        app:layout_constraintStart_toStartOf="@+id/vert1"
        app:layout_constraintTop_toTopOf="@+id/horiz3"
        app:layout_constraintVertical_bias="0.6" />

    <TextView
        android:id="@+id/cadenceTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/fw_offset"
        android:textAppearance="@android:style/TextAppearance.Material.Medium"
        app:layout_constraintTop_toTopOf="@+id/horiz3"
        app:layout_constraintBottom_toTopOf="@+id/cadenceValueTV"
        app:layout_constraintEnd_toStartOf="@+id/vert3"
        app:layout_constraintStart_toStartOf="@+id/vert2" />

    <TextView
        android:id="@+id/cadenceValueTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="32sp"
        android:text="@{String.valueOf(tsdz_debug.fwOffset)}"
        android:textAppearance="@android:style/TextAppearance.Material.Large"
        app:layout_constraintBottom_toTopOf="@+id/horiz4"
        app:layout_constraintEnd_toStartOf="@+id/vert3"
        app:layout_constraintStart_toStartOf="@+id/vert2"
        app:layout_constraintTop_toTopOf="@+id/horiz3"
        app:layout_constraintVertical_bias="0.6"/>


    <TextView
        android:id="@+id/adcTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/torque_smooth"
        android:textAppearance="@android:style/TextAppearance.Material.Medium"
        app:layout_constraintTop_toTopOf="@+id/horiz4"
        app:layout_constraintBottom_toTopOf="@+id/adcValueTV"
        app:layout_constraintEnd_toStartOf="@+id/vert2"
        app:layout_constraintStart_toStartOf="@+id/vert1" />

    <TextView
        android:id="@+id/adcValueTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="32sp"
        android:text="@{String.valueOf(tsdz_debug.torqueSmoothPct)}"
        android:textAppearance="@android:style/TextAppearance.Material.Large"
        app:layout_constraintBottom_toTopOf="@+id/horiz5"
        app:layout_constraintEnd_toStartOf="@+id/vert2"
        app:layout_constraintStart_toStartOf="@+id/vert1"
        app:layout_constraintTop_toTopOf="@+id/horiz4"
        app:layout_constraintVertical_bias="0.6" />

    <TextView
        android:id="@+id/pcbTempTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/controller_temp"
        android:textAppearance="@android:style/TextAppearance.Material.Medium"
        app:layout_constraintTop_toTopOf="@+id/horiz4"
        app:layout_constraintBottom_toTopOf="@+id/pcbTempValueTV"
        app:layout_constraintEnd_toStartOf="@+id/vert3"
        app:layout_constraintStart_toStartOf="@+id/vert2" />

    <TextView
        android:id="@+id/pcbTempValueTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="32sp"
        android:text="@{@string/temperatureField(tsdz_debug.pcbTemperature)}"
        android:textAppearance="@android:style/TextAppearance.Material.Large"
        app:layout_constraintBottom_toTopOf="@+id/horiz5"
        app:layout_constraintEnd_toStartOf="@+id/vert3"
        app:layout_constraintStart_toStartOf="@+id/vert2"
        app:layout_constraintTop_toTopOf="@+id/horiz4"
        app:layout_constraintVertical_bias="0.6" />
</androidx.constraintlayout.widget.ConstraintLayout>
</layout>