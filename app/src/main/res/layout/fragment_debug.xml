<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".fragments.FragmentDebug">
    <data>
        <variable name="tsdz_debug" type="spider65.ebike.tsdz2_esp32.fragments.FragmentDebug.FragmentData"/>
    </data>

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/vert1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/vert2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/vert3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="1" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/horiz1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/horiz2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.25" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/horiz3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.5" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/horiz4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.75" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/horiz5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="1" />

    <FrameLayout
        android:id="@+id/fl11"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/border"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/fl12"
        app:layout_constraintBottom_toTopOf="@+id/horiz2"
        app:layout_constraintTop_toTopOf="@+id/horiz1">
    </FrameLayout>
    <FrameLayout
        android:id="@+id/fl12"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/border"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/fl11"
        app:layout_constraintBottom_toTopOf="@+id/horiz2"
        app:layout_constraintTop_toTopOf="@+id/horiz1">
    </FrameLayout>

    <FrameLayout
        android:id="@+id/fl21"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/border"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/fl22"
        app:layout_constraintBottom_toTopOf="@+id/horiz3"
        app:layout_constraintTop_toTopOf="@+id/horiz2">
    </FrameLayout>
    <FrameLayout
        android:id="@+id/fl22"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/border"
        app:layout_constraintBottom_toTopOf="@+id/horiz3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/fl21"
        app:layout_constraintTop_toTopOf="@+id/horiz2">
    </FrameLayout>
    <FrameLayout
        android:id="@+id/fl31"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/border"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/fl32"
        app:layout_constraintBottom_toTopOf="@+id/horiz4"
        app:layout_constraintTop_toTopOf="@+id/horiz3">
    </FrameLayout>
    <FrameLayout
        android:id="@+id/fl32"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/border"
        app:layout_constraintBottom_toTopOf="@+id/horiz4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/fl31"
        app:layout_constraintTop_toTopOf="@+id/horiz3">
    </FrameLayout>
    <FrameLayout
        android:id="@+id/fl41"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/border"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/fl42"
        app:layout_constraintBottom_toTopOf="@+id/horiz5"
        app:layout_constraintTop_toTopOf="@+id/horiz4">
    </FrameLayout>
    <FrameLayout
        android:id="@+id/fl42"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/border"
        app:layout_constraintBottom_toTopOf="@+id/horiz5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/fl41"
        app:layout_constraintTop_toTopOf="@+id/horiz4">
    </FrameLayout>
    <!--
    <com.shinelw.library.ColorArcProgressBar
        android:id="@+id/bar1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@+id/horiz1"
        app:layout_constraintBottom_toBottomOf="@+id/horiz2"
        app:layout_constraintEnd_toStartOf="@+id/vert2"
        app:layout_constraintStart_toStartOf="@+id/vert1"
        android:layout_gravity="center_horizontal"
        app:is_need_content="true"
        app:front_color1="#00ff00"
        app:front_color2="#ffff00"
        app:front_color3="#ff0000"
        app:max_value="100"
        app:back_width="2dp"
        app:front_width="10dp"
        app:text_size="38sp"
        app:total_engle="270"
        app:is_need_unit="true"
        app:string_unit="%"
